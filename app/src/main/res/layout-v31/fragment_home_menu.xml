<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:layout_marginBottom="20dp"
            android:id="@+id/fr_home_menu"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"
                android:orientation="vertical"
                tools:context=".HomeMenuFragment">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="400dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="20dp"
                    android:background="@color/white">

                    <ImageView
                        android:id="@+id/iv_poster"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/rounded_square_20dp_width2dp"
                        android:clipToOutline="true"
                        android:scaleType="fitXY"
                        android:src="@drawable/viewpager1" />

                </FrameLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/white">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cl_menu"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white">

                        <ImageView
                            android:id="@+id/ib_map"
                            android:layout_width="150dp"
                            android:layout_height="220dp"
                            android:layout_margin="20dp"
                            android:background="@drawable/rounded_square_20dp_width2dp"
                            android:clipToOutline="true"
                            android:scaleType="fitXY"
                            android:src="@drawable/maps"
                            app:layout_constraintStart_toStartOf="@id/cl_menu"
                            app:layout_constraintTop_toTopOf="@id/cl_menu" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="10dp"
                            android:background="@drawable/rounded_square_20dp_width2dp"
                            android:backgroundTint="@color/white"
                            android:padding="5dp"
                            android:text="복지 시설 찾기"
                            android:textSize="8dp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="@id/ib_map"
                            app:layout_constraintEnd_toEndOf="@id/ib_map" />


                        <ImageView
                            android:id="@+id/ib_dic"
                            android:layout_width="200dp"
                            android:layout_height="105dp"
                            android:layout_marginEnd="20dp"
                            android:contentDescription="TODO"
                            android:src="@drawable/rounded_square_20dp_width2dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="1.0"
                            app:layout_constraintStart_toEndOf="@+id/ib_map"
                            app:layout_constraintTop_toTopOf="@+id/ib_map" />

                        <ImageView
                            android:id="@+id/ib_youtube"
                            android:layout_width="200dp"
                            android:layout_height="105dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginEnd="20dp"
                            android:background="@drawable/rounded_square_20dp_width2dp"
                            android:clipToOutline="true"
                            android:contentDescription="TODO"
                            android:scaleType="fitXY"
                            android:src="@drawable/ib_youtube_image"
                            app:layout_constraintBottom_toBottomOf="@+id/ib_map"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="1.0"
                            app:layout_constraintStart_toEndOf="@+id/ib_map"
                            app:layout_constraintTop_toBottomOf="@+id/ib_dic" />

                        <ImageView
                            android:id="@+id/ib_vib"
                            android:layout_width="110dp"
                            android:layout_height="110dp"
                            android:background="@drawable/rounded_square_20dp_width2dp"
                            android:padding="20dp"
                            android:paddingBottom="10dp"
                            android:scaleType="centerInside"
                            android:src="@drawable/ic_vibration_vector"
                            app:layout_constraintBottom_toBottomOf="@+id/imageButton6"
                            app:layout_constraintStart_toStartOf="@id/ib_map"
                            app:layout_constraintTop_toTopOf="@+id/imageButton6" />


                        <ImageView
                            android:id="@+id/imageButton5"
                            android:layout_width="110dp"
                            android:layout_height="110dp"
                            android:src="@drawable/rounded_square_20dp_width2dp"
                            app:layout_constraintBottom_toBottomOf="@+id/imageButton6"
                            app:layout_constraintEnd_toEndOf="@+id/ib_youtube"
                            app:layout_constraintTop_toTopOf="@+id/imageButton6" />

                        <ImageView
                            android:id="@+id/imageButton6"
                            android:layout_width="110dp"
                            android:layout_height="110dp"
                            android:layout_marginTop="20dp"
                            android:src="@drawable/rounded_square_20dp_width2dp"
                            app:layout_constraintEnd_toStartOf="@+id/imageButton5"
                            app:layout_constraintStart_toEndOf="@+id/ib_vib"
                            app:layout_constraintTop_toBottomOf="@+id/ib_map" />


                    </androidx.constraintlayout.widget.ConstraintLayout>
                </LinearLayout>


            </LinearLayout>
        </FrameLayout>
    </androidx.core.widget.NestedScrollView>
</layout>
